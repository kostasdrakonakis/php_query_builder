<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#835C3B" />
            <stroke
                android:width="1dp"
                android:color="#835C3B" />
            <corners
                android:radius="0dp" />

        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#835C3B"
                android:endColor="#835C3B"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#C2B280" />
            <corners
                android:radius="0dp" />

        </shape>
    </item>
</selector>